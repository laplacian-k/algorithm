#include <string>
#include <vector>

using namespace std;

vector<string> solution(int n, vector<int> arr1, vector<int> arr2) {
    // 하나라도 1이라면 1
    // 둘다 0이라면 0
    
    vector<string> answer(n);
    
    // 0 : 공백 1: 벽
    
    // map을 만듦.    
    vector<int> map(n,0);
    for(int i = 0; i < n; i++) {
        map[i] = (arr1[i] | arr2[i]);
        string str = "";
        for(int j = n-1; j >= 0; j--){
            
            // 0인지 1인지 판단
            // 벽이라면
            if( (map[i] & (1<<j)) != 0)
                str += "#";
            else
                str += " ";           
        }
        answer[i] = str;
    }    
    
    return answer;
}
