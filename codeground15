#include <iostream>

using namespace std;

// variables
int N, M;
bool fee[1001];
bool visit[1001];
int path[1001][1001];

int main(int argc, char** argv)
{
	int T, test_case;
	
	cin >> T;
	for(test_case = 0; test_case  < T; test_case++)
	{
        cin >> N >> M;
        for(int i = 0; i < M; i++) {
            int from, to, weight;
            cin >> from >> to >> weight;
            path[from][to] = weight;
        }
        
		
		cout << "Case #" << test_case+1 << endl;
		
		for(int i = 1; i <= N; i++ ) {
		    if(!fee[i])
		        cout << i << " ";
		}
	}

	return 0;//Your program should return 0 on normal termination.
}
